# AccessFi Deployment Configuration
# Copy this file to .env and fill in your actual values

# ZK Verify Contract Address (REQUIRED - replace with actual address)
ZK_VERIFY_ADDRESS=

# Verification Key for ZK proofs (REQUIRED - replace with actual key)
VERIFICATION_KEY=

# NFT Configuration
NFT_NAME="AccessFi Proof NFT"
NFT_SYMBOL="AFPNFT"

# Deployment Options
CREATE_INITIAL_POOL=true

# RPC URLs (uncomment and fill for your networks)
# MAINNET_RPC_URL=
# GOERLI_RPC_URL=
# SEPOLIA_RPC_URL=
# POLYGON_RPC_URL=

# Private Keys (for deployment - keep secure!)
# PRIVATE_KEY=
# DEPLOYER_PRIVATE_KEY=

# Contract Addresses (filled automatically after deployment)
# VERIFY_PROOF_ADDRESS=
# FACTORY_ADDRESS=
# FIRST_POOL_ADDRESS=